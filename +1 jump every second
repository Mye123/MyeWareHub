local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
local Window = Rayfield:CreateWindow({
	Name = "MyeWare Hub",
	LoadingTitle = "+1 jump every second",
	LoadingSubtitle = "by Mye",
	ConfigurationSaving = {
		Enabled = true,
		FolderName = nil, -- Create a custom folder for your hub/game
		FileName = "Big Hub"
	},
        Discord = {
        	Enabled = true,
        	Invite = "vjjcYyCpAg", -- The Discord invite code, do not include discord.gg/
        	RememberJoins = true -- Set this to false to make them join the discord every time they load it up
        },
	KeySystem = true, -- Set this to true to use our key system
	KeySettings = {
		Title = "MyeWare Hub",
		Subtitle = "Key System",
		Note = "Join the discord for key , (you have been invited)",
		FileName = "KeySave",
		SaveKey = true,
		GrabKeyFromSite = false, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
		Key = "yes."
	}
})
local Tab = Window:CreateTab("Main", 4483362458)
local SpawnSection = Tab:CreateSection("Spawn")
local Button = Tab:CreateButton({
	Name = "Rainbow tower (small) (1 win)",
	Callback = function()
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1.80389, 7850.14, 8010)-- The function that takes place when the button is pressed
	end,
})
